article.markdown-body
  h1= @content.title
  = raw @content.markdown
  span.author written by
  = link_to @content.user.profile.name, profile_path(@content.user)
  - if current_user.owner?(@content)
    = link_to 'Edit', edit_user_content_path(user_id: current_user, id: @content), class: 'btn btn-primary'
= javascript_include_tag 'custom'
